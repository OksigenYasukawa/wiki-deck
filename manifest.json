{
    "name": "Wiki Deck",
    "description": "Untuk memudahkan dalam web Fandom dan keren",
    "version": "0.0.0.2",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [                
                "https://*.minecraftid.fandom.com/*",                
                "https://*.brutal.fandom.com/*",
                "https://*.twitter.com/*",
                "https://*.youtube.com/*",
                "https://*.instagram.com/*",
                "https://*.open.spotify.com/*",
                "https://*.web.whatsapp.com/*",
                "https://*.docs.google.com/*"
            ],
            "js": [
                "app.js"
            ]
        }
    ],
    "icons": {
        "48": "PP.png",
        "128": "PP.png"
    }
}